logout) {\r\n      props.history.push(\"/\");\r\n    }\r\n  }, [logout]);\r\n\r\n  return (\r\n    <>\r\n      {token.length > 0 && (\r\n        <div>\r\n          <NavBarTest setLogout={setLogout} />\r\n\r\n          <div className=\"container mt-4\">\r\n            <div id=\"portada\" className=\"row\">\r\n              <div className=\"col\">\r\n                <img src={Portada} alt=\"portada\" />\r\n              </div>\r\n            </div>\r\n            <div className=\"row mb-4\">\r\n              <div className=\"col\">\r\n                <TablaProductos usuario={usuario} />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n","/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/helpers/Usuario.js",[],"/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/FormRegistro.jsx",[],"/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/NavBarTest.jsx",["67"],"import React, { useEffect, useState } from \"react\";\r\n\r\nimport { Navbar, Nav, Form, Button } from \"react-bootstrap\";\r\nimport \"../css/tabla.css\";\r\nimport Logo from \"../images/logo.png\";\r\nexport default function NavBarTest({ setLogout }) {\r\n  const [token, setToken] = useState(\"\");\r\n\r\n  useEffect(() => {\r\n    setToken(JSON.parse(localStorage.getItem(\"token\")) || \"\");\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <Navbar bg=\"dark\" variant=\"dark\" expand=\"lg\">\r\n        <div className=\"container\">\r\n          <Navbar.Brand>\r\n            <img id=\"imagenLogo\" src={Logo} alt=\"logo\" />\r\n          </Navbar.Brand>\r\n          <Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\r\n          <Navbar.Collapse id=\"basic-navbar-nav\">\r\n            <Nav className=\"ml-auto mr-2\">\r\n              {/* <Nav.Link href=\"#home\">\r\n              <i className=\"fas ad\"></i>\r\n              Home\r\n            </Nav.Link>\r\n            <Nav.Link href=\"#link\">Link</Nav.Link> */}\r\n            </Nav>\r\n            <Form inline>\r\n              <Button\r\n                variant=\"outline-info\"\r\n                onClick={() => {\r\n                  setToken(\"\");\r\n                  localStorage.clear();\r\n                  setLogout(true);\r\n                }}\r\n              >\r\n                <i className=\"fa fa-sign-out\" aria-hidden=\"true\"></i>\r\n              </Button>\r\n              {/* {token === \"\" ? (\r\n              <Button variant=\"outline-success\" onClick={handleShow}>\r\n                Loguearse\r\n              </Button>\r\n            ) : (\r\n              <Button\r\n                variant=\"outline-secondary\"\r\n                onClick={() => {\r\n                  setToken(\"\");\r\n                  localStorage.clear();\r\n                 \r\n                }}\r\n              >\r\n                Salir\r\n              </Button>\r\n              // <span className=\"text-white\">Hello!</span>\r\n            )} */}\r\n            </Form>\r\n          </Navbar.Collapse>\r\n        </div>\r\n      </Navbar>\r\n      {/* <ModalTest handleClose={handleClose} show={show} /> */}\r\n    </>\r\n  );\r\n}\r\n","/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/TablaProductos.jsx",[],"/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/ModalTest.jsx",[],"/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/FormUpdateProd.jsx",["68"],"/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/modales/MensajeDelete.jsx",[],"/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/component/modales/FormAddProducto.jsx",["69"],"import React, { useEffect, useState } from \"react\";\r\nimport { Form, Button, Modal, Alert } from \"react-bootstrap\";\r\n\r\nexport default function FormAddProducto(props) {\r\n  let token = JSON.parse(localStorage.getItem(\"token\"));\r\n\r\n  const [state, setState] = useState({\r\n    form: {\r\n      nombre: \"\",\r\n      precioUni: \"\",\r\n      descripcion: \"\",\r\n      categoria: \"\",\r\n    },\r\n  });\r\n\r\n  const [estado, setEstados] = useState({\r\n    loading: false,\r\n    error: null,\r\n  });\r\n  const [cat, setCat] = useState([]);\r\n\r\n  useEffect(() => {\r\n    getCategoria();\r\n  }, []);\r\n\r\n  const handleChange = ({ target }) => {\r\n    setState({\r\n      form: {\r\n        ...state.form,\r\n        [target.name]: target.value,\r\n      },\r\n    });\r\n  };\r\n\r\n  const getCategoria = async () => {\r\n    try {\r\n      const resp = await fetch(\"http://localhost:3005/categoria\", {\r\n        method: \"GET\",\r\n        headers: {\r\n          \"Content-type\": \"application/json; charset=UTF-8\",\r\n          token: `${token}`,\r\n        },\r\n      });\r\n      const data = await resp.json();\r\n      console.log(data.categorias);\r\n      setCat(data.categorias);\r\n    } catch (error) {\r\n      console.log(error);\r\n    }\r\n  };\r\n\r\n  const postProducto = async (e) => {\r\n    e.preventDefault();\r\n    setEstados({\r\n      loading: true,\r\n      error: null,\r\n    });\r\n    try {\r\n      const resp = await fetch(`http://localhost:3005/producto`, {\r\n        method: \"POST\",\r\n        body: JSON.stringify(state.form),\r\n        headers: {\r\n          \"Content-type\": \"application/json; charset=UTF-8\",\r\n          token: `${token}`,\r\n        },\r\n      });\r\n      const data = await resp.json();\r\n      console.log(data);\r\n      setEstados({\r\n        loading: false,\r\n        error: null,\r\n      });\r\n      if (data.ok) {\r\n        props.actualizaLista(props.page);\r\n        props.handleClose();\r\n      } else {\r\n        setEstados({\r\n          loading: false,\r\n          error: data.ok,\r\n        });\r\n      }\r\n    } catch (error) {\r\n      console.log(error);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      <Modal.Header closeButton>\r\n        <Modal.Title>Nuevo Producto</Modal.Title>\r\n      </Modal.Header>\r\n      <Modal.Body>\r\n        <Form onSubmit={postProducto}>\r\n          <Form.Group>\r\n            <Form.Label>Nombre</Form.Label>\r\n            <Form.Control\r\n              type=\"text\"\r\n              placeholder=\"Ingrese el nombre del producto\"\r\n              value={state.form.nombre}\r\n              name=\"nombre\"\r\n              onChange={handleChange}\r\n              required\r\n              disabled={estado.loading ? true : false}\r\n            />\r\n          </Form.Group>\r\n\r\n          <Form.Group>\r\n            <Form.Label>Descripción</Form.Label>\r\n            <Form.Control\r\n              type=\"text\"\r\n              placeholder=\"Breve descripción del producto\"\r\n              value={state.form.descripcion}\r\n              name=\"descripcion\"\r\n              onChange={handleChange}\r\n              disabled={estado.loading ? true : false}\r\n            />\r\n          </Form.Group>\r\n\r\n          <Form.Group>\r\n            <Form.Label>Categoría</Form.Label>\r\n            <Form.Control\r\n              as=\"select\"\r\n              name=\"categoria\"\r\n              onChange={handleChange}\r\n              placeholder=\"Elige la categoría\"\r\n              required\r\n              disabled={estado.loading ? true : false}\r\n            >\r\n              <option>Elige la categoría</option>\r\n              {cat.map((categoria) => (\r\n                <option key={categoria._id} value={categoria._id}>\r\n                  {categoria.descripcion}\r\n                </option>\r\n              ))}\r\n            </Form.Control>\r\n          </Form.Group>\r\n          <Form.Group>\r\n            <div className=\"col-6 offset-6 \">\r\n              <Form.Label>Precio</Form.Label>\r\n              <Form.Control\r\n                type=\"number\"\r\n                value={state.form.precioUni}\r\n                name=\"precioUni\"\r\n                onChange={handleChange}\r\n                required\r\n                disabled={estado.loading ? true : false}\r\n              />\r\n            </div>\r\n          </Form.Group>\r\n          {estado.error === false && (\r\n            <div>\r\n              <Alert variant=\"danger\" className=\"text-center\">\r\n                Verificar los datos\r\n              </Alert>\r\n            </div>\r\n          )}\r\n          <hr />\r\n          <div className=\"col-6 offset-6\">\r\n            <Button variant=\"info\" type=\"submit\" block>\r\n              Guardar\r\n            </Button>\r\n          </div>\r\n        </Form>\r\n      </Modal.Body>\r\n      {/* <Modal.Footer>\r\n        <Button variant=\"secondary\" onClick={props.handleClose}>\r\n          No\r\n        </Button>\r\n        <Button variant=\"danger\" onClick={deleteProductos}>\r\n          Yes\r\n        </Button>\r\n      </Modal.Footer> */}\r\n    </>\r\n  );\r\n}\r\n","/mnt/c/Users/pablo/OneDrive/Documentos/bootstrap-react-app/src/helpers/Productos.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":24,"column":6,"nodeType":"76","endLine":24,"endColumn":15,"suggestions":"77"},{"ruleId":"74","severity":1,"message":"78","line":40,"column":6,"nodeType":"76","endLine":40,"endColumn":8,"suggestions":"79"},{"ruleId":"74","severity":1,"message":"75","line":47,"column":6,"nodeType":"76","endLine":47,"endColumn":14,"suggestions":"80"},{"ruleId":"81","severity":1,"message":"82","line":7,"column":10,"nodeType":"83","messageId":"84","endLine":7,"endColumn":15},{"ruleId":"74","severity":1,"message":"85","line":23,"column":6,"nodeType":"76","endLine":23,"endColumn":8,"suggestions":"86"},{"ruleId":"74","severity":1,"message":"87","line":24,"column":6,"nodeType":"76","endLine":24,"endColumn":8,"suggestions":"88"},"no-native-reassign",["89"],"no-negated-in-lhs",["90"],"react-hooks/exhaustive-deps","React Hook useEffect has a miss
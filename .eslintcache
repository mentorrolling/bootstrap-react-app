/\");\r\n    }\r\n  }, [logout]);\r\n\r\n  return (\r\n    <>\r\n      {token.length > 0 && (\r\n        <div>\r\n          <NavBarTest setLogout={setLogout} />\r\n\r\n          <div className=\"container mt-4\">\r\n            <div id=\"portada\" className=\"row\">\r\n              <div className=\"col\">\r\n                <img src={Portada} alt=\"portada\" />\r\n              </div>\r\n            </div>\r\n            <div className=\"row mb-4\">\r\n              <div className=\"col\">\r\n                <TablaProductos usuario={usuario} />\r\n              </div>\r\n            </div>\r\n            <div className=\"row mb-4\">\r\n              <div className=\"col-12 col-md-8 offset-md-2\">\r\n                <TablaCategoria usuario={usuario} />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      )}\r\n    </>\r\n  );\r\n}\r\n","C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\pages\\Login.jsx",["93"],"C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\helpers\\Usuario.js",[],"C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\component\\FormRegistro.jsx",[],"C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\component\\NavBarTest.jsx",["94"],"import React, { useEffect, useState } from \"react\";\r\n\r\nimport { Navbar, Nav, Form, Button } from \"react-bootstrap\";\r\nimport \"../css/tabla.css\";\r\nimport Logo from \"../images/logo.png\";\r\nexport default function NavBarTest({ setLogout }) {\r\n  const [token, setToken] = useState(\"\");\r\n\r\n  useEffect(() => {\r\n    setToken(JSON.parse(localStorage.getItem(\"token\")) || \"\");\r\n  }, []);\r\n\r\n  return (\r\n    <>\r\n      <Navbar bg=\"dark\" variant=\"dark\" expand=\"lg\">\r\n        <div className=\"container\">\r\n          <Navbar.Brand>\r\n            <img id=\"imagenLogo\" src={Logo} alt=\"logo\" />\r\n          </Navbar.Brand>\r\n          <Navbar.Toggle aria-controls=\"basic-navbar-nav\" /
orm.Control\r\n            name=\"precio\"\r\n            type=\"text\"\r\n            value={actualizado.precioUni}\r\n            onChange={handleChange}\r\n          />\r\n        </InputGroup>\r\n      </Form.Group>\r\n      <Form.Group>\r\n        <Form.Label>Descripción</Form.Label>\r\n        <Form.Control\r\n          name=\"descripcion\"\r\n          type=\"text\"\r\n          value={actualizado.descripcion}\r\n          onChange={handleChange}\r\n        />\r\n      </Form.Group>\r\n      <div className=\"d-flex justify-content-end\">\r\n        <Button variant=\"primary\" type=\"submit\" onClick={props.handleClose}>\r\n          <i className=\"fa fa-refresh\" aria-hidden=\"true\"></i> Actualizar\r\n        </Button>\r\n      </div>\r\n    </Form>\r\n  );\r\n}\r\n","C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\pages\\Error404.jsx",[],"C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\component\\TablaProductos.jsx",["63","64"],"C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\component\\modales\\MensajeDelete.jsx",[],"C:\\Users\\pablo\\OneDrive\\Documentos\\bootstrap-react-app\\src\\component\\modales\\FormAddProducto.jsx",["65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":7,"column":10,"nodeType":"72","messageId":"73","endLine":7,"endColumn":15},{"ruleId":"74","severity":1,"message":"75","line":34,"column":6,"nodeType":"76","endLine":34,"endColumn":8,"suggestions":"77"},{"ruleId":"74","severity":1,"message":"78","line":41,"column":6,"nodeType":"76","endLine":41,"endColumn":14,"suggestions":"79"},{"ruleId":"74","severity":1,"message":"78","line":24,"column":6,"nodeType":"76","endLine":24,"endColumn":15,"suggestions":"80"},{"ruleId":"81","severity":1,"message":"82","line":32,"column":9,"nodeType":"83","endLine":32,"endColumn":46},{"ruleId":"74","severity":1,"message":"84","line":23,"column":6,"nodeType":"76","endLine":23,"endColumn"